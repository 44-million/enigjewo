{
    "presets": [
        "@babel/preset-env",
        [
            "@babel/preset-react",
            {
                "runtime": "automatic"
            }
        ]
    ],
    "plugins": [
        [
            "module-resolver",
            {
                "root": ["./src"],
                "alias": {
                    "core": "./src/core",
                    "hooks": "./src/core/hooks",
                    "components": "./src/components",
                    "containers": "./src/containers",
                    "styles": "./src/styles"
                }
            }
        ]
    ],
    "compact": false,
    "comments": false
}
